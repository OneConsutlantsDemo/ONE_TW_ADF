{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "one-datafactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/PL_ONE_TW_DATA_REFRESH')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Extract",
						"type": "ExecutePipeline",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "PL_ONE_TW_EXTRACT_ETL",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true
						}
					},
					{
						"name": "Transform",
						"type": "ExecutePipeline",
						"dependsOn": [
							{
								"activity": "Extract",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"pipeline": {
								"referenceName": "PL_ONE_TW_TRANSFORMATION_ETL",
								"type": "PipelineReference"
							},
							"waitOnCompletion": true
						}
					},
					{
						"name": "WebHook_Compliance_Tabular_Model",
						"type": "WebHook",
						"dependsOn": [
							{
								"activity": "Transform",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"url": "https://s2events.azure-automation.net/webhooks?token=1RQYI4hsyYOS2wWgyjGfkCpJRNK1btdof0NIhFVrPsw%3d",
							"method": "POST",
							"headers": {
								"Content-Type": {
									"value": "application/json",
									"type": "Expression"
								}
							},
							"body": {
								"AnalysisServicesDatabaseName": "ONE_MODEL_TW_Complaince",
								"AnalysisServicesServer": "asazure://westeurope.asazure.windows.net/oneanalysis",
								"DatabaseRefreshType": "Full"
							},
							"timeout": "00:10:00"
						}
					},
					{
						"name": "WebHook_Expense_Tabular_Model",
						"type": "WebHook",
						"dependsOn": [
							{
								"activity": "WebHook_Compliance_Tabular_Model",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"url": "https://s2events.azure-automation.net/webhooks?token=1RQYI4hsyYOS2wWgyjGfkCpJRNK1btdof0NIhFVrPsw%3d",
							"method": "POST",
							"body": {
								"AnalysisServicesDatabaseName": "ONE_MODEL_TW_Expense",
								"AnalysisServicesServer": "asazure://westeurope.asazure.windows.net/oneanalysis",
								"DatabaseRefreshType": "Full"
							},
							"timeout": "00:10:00"
						}
					}
				],
				"folder": {
					"name": "ONE_TIMESHEET_PROJECT"
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "WebHook_Compliance_Tabular_Model",
						"type": "WebHook",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"url": "https://s2events.azure-automation.net/webhooks?token=1RQYI4hsyYOS2wWgyjGfkCpJRNK1btdof0NIhFVrPsw%3d",
							"method": "POST",
							"headers": {
								"Content-Type": "application/json"
							},
							"body": {
								"AnalysisServicesDatabaseName": "ONE_MODEL_TW_Complaince",
								"AnalysisServicesServer": "asazure://westeurope.asazure.windows.net/oneanalysis",
								"DatabaseRefreshType": "Full"
							},
							"timeout": "00:10:00"
						}
					},
					{
						"name": "WebHook_Expense_Tabular_Model",
						"type": "WebHook",
						"dependsOn": [
							{
								"activity": "WebHook_Compliance_Tabular_Model",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"url": "https://s2events.azure-automation.net/webhooks?token=1RQYI4hsyYOS2wWgyjGfkCpJRNK1btdof0NIhFVrPsw%3d",
							"method": "POST",
							"body": {
								"AnalysisServicesDatabaseName": "ONE_MODEL_TW_Expense",
								"AnalysisServicesServer": "asazure://westeurope.asazure.windows.net/oneanalysis",
								"DatabaseRefreshType": "Full"
							},
							"timeout": "00:10:00"
						}
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
}