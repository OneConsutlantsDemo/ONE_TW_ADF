{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "one-datafactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline4')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ML Batch Execution1",
						"type": "AzureMLBatchExecution",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"webServiceInputs": {
								"R&D Spend": {
									"filePath": "onefilesystemgen2/1000_Companies.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								},
								"Administration": {
									"filePath": "onefilesystemgen2/1000_Companies.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								},
								"Marketing Spend": {
									"filePath": "onefilesystemgen2/1000_Companies.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								},
								"State": {
									"filePath": "onefilesystemgen2/1000_Companies.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								},
								"Profit": {
									"filePath": "onefilesystemgen2/1000_Companies.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								}
							},
							"webServiceOutputs": {
								"State": {
									"filePath": "onefilesystemgen2/Demo.csv",
									"linkedServiceName": {
										"referenceName": "AzureBlobStorageML",
										"type": "LinkedServiceReference"
									}
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "AzureML1",
							"type": "LinkedServiceReference"
						}
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
}