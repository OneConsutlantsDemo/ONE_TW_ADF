{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "one-datafactory"
		},
		"AzureBlobStorageML_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageML'"
		},
		"AzureML1_apiKey": {
			"type": "secureString",
			"metadata": "Secure string for 'apiKey' of 'AzureML1'"
		},
		"SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDB'"
		},
		"SapHanaOne_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SapHanaOne'"
		},
		"ADLS_Gen1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://onedatalakeadls.azuredatalakestore.net/webhdfs/v1"
		},
		"ADLS_Gen1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "8a7bb8e8-a0ba-4b1a-95cb-5169eb7ae0f0"
		},
		"ADLS_Gen1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "11ebd399-8f6c-46af-b26f-71fc4572151b"
		},
		"ADLS_Gen1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "ONE-Azure"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageML_connectionString": {
						"value": "[parameters('AzureBlobStorageML_connectionString')]"
					},
					"AzureML1_apiKey": {
						"value": "[parameters('AzureML1_apiKey')]"
					},
					"SQLDB_connectionString": {
						"value": "[parameters('SQLDB_connectionString')]"
					},
					"ADLS_Gen1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('ADLS_Gen1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"ADLS_Gen1_properties_typeProperties_tenant": {
						"value": "[parameters('ADLS_Gen1_properties_typeProperties_tenant')]"
					},
					"ADLS_Gen1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ADLS_Gen1_properties_typeProperties_subscriptionId')]"
					},
					"ADLS_Gen1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ADLS_Gen1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SapHanaOne_connectionString": {
						"value": "[parameters('SapHanaOne_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		},
		{
			"name": "ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_2')]"
			]
		},
		{
			"name": "ArmTemplate_4",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_4.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_3')]"
			]
		}
	]
}