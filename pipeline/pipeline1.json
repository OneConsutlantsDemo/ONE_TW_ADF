					"source": {
						"type": "OracleSource",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						}
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"dataIntegrationUnits": 16
				},
				"inputs": [
					{
						"referenceName": "OracleDemo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "TargetGen2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}